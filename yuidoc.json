{
  "name": "Spies",
  "description": "A simple library to spy on functions and create stubs for unit testing.",
  "version": "0.1.4",
  "url": "https://github.com/garysoed/spies",
  "colorMain": "#004000",
  "colorMainLight": "#008000",
  "colorKeyword": "#808080",
  "colorLink": "gold",
  "colorLinkVisited": "darkorange",
  "options": {
    "linkNatives": "true",
    "outdir": "./doc",
    "themedir": "./node_modules/doctheme",
    "paths": [
      "./src"
    ],
    "external": {
      "data": {
        "base": "../assets/redirect.html#",
        "json": "./node_modules/doctheme/externals/dom.json"
      }
    }
  }
}